import random

# Генерация списка из 10 случайных чисел
random_list = [random.randint(1, 100) for _ in range(10)]

# Вывод списка на экран
print("Сгенерированный список:", random_list)

# Сортировка списка
sorted_list = sorted(random_list)

# Вывод минимального и максимального значений
min_value = sorted_list[0]
max_value = sorted_list[-1]
print("Минимальное значение:", min_value)
print("Максимальное значение:", max_value)

# Подсчет суммы всех чисел в списке
total_sum = sum(random_list)
print("Сумма всех чисел в списке:", total_sum)
